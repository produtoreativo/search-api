{
  "name": "mssql-connector", 
  "config": {
      "connector.class": "io.debezium.connector.sqlserver.SqlServerConnector", 
      "database.server.name": "mssql",
      "database.hostname": "mssql", 
      "database.port": "1433", 
      "database.user": "sa", 
      "database.password": "@Fuh1478", 
      "database.names": "solar", 
      "topic.prefix": "fullfillment", 
      "table.include.list": "dbo.products", 
      "schema.history.internal.kafka.bootstrap.servers": "kafka:9092", 
      "schema.history.internal.kafka.topic": "schemahistory.fullfillment",
      "driver.trustServerCertificate": "true",
      "transforms.reroute_topic.type": "io.debezium.transforms.ByLogicalTableRouter",
      "transforms.reroute_topic.key.enforce.uniqueness": "false",
      "transforms.reroute_topic.topic.regex": "products_reroute_source_topic",
      "transforms.reroute_topic.topic.replacement": "products_reroute_target_topic",
      "transforms": "unwrap,reroute_topic",
      "transforms.unwrap.type": "io.debezium.transforms.ExtractNewRecordState",
      "transforms.unwrap.drop.tombstones": "false"
  }
}